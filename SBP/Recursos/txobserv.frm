End Sub

Sub Carga_Familia()

    Dim mytablex As New ADODB.Recordset

    Dim I        As Integer

    For I = 0 To 14999
        mfamcod(I) = ""
        ''' kenyo 09/11/2017 Mejora grupo comentario
        mfamdes(I) = ""
        ''' kenyo 09/11/2017 Mejora grupo comentario
    Next I

    I = -1

    '' kenyo 09/11/2017 Mejora grupo comentario
    'mytablex.Open "select * from grupoco ", cn, adOpenStatic, adLockOptimistic
    mytablex.Open "select grupoco,substring(descripcio,1,len(descripcio)) as descripcio from grupoco order by descripcio ", cn, adOpenStatic, adLockOptimistic
    '' kenyo 09/11/2017 Mejora grupo comentario

    Do

        If mytablex.EOF Then Exit Do
        I = I + 1

        ''' kenyo 09/11/2017 Mejora grupo comentario
        'mfamcod(i) = "" & mytablex.Fields("grupoco")
        mfamcod(I) = "" & mytablex.Fields("grupoco")
        mfamdes(I) = "" & mytablex.Fields("descripcio")
        ''' kenyo 09/11/2017 Mejora grupo comentario

        mytablex.MoveNext
    Loop
    mfamtop = I
    mytablex.Close
    mfampag = 0
    menu_familia "INI"

End Sub

