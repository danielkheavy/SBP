Sub menu_carga_producto(buf As String)

    Dim mytablex As New ADODB.Recordset

    Dim I        As Integer

    For I = 0 To 11
        wwprodcod(I) = ""
    Next I

    For I = 0 To 14999
        mprodcod(I) = ""
        wprodcod(I) = ""
    Next I

    I = -1

    If mytablex.State = 1 Then mytablex.Close

    ''' kenyo 09/11/2017 Mejora grupo comentario
    'mytablex.Open "SELECT * FROM comentario where grupoco='" & buf & "' order by comentario ", cn, adOpenDynamic, adLockOptimistic
    mytablex.Open "SELECT grupoco,comentario,substring(descripcio,1,len(descripcio)) as descripcio FROM comentario where grupoco='" & buf & "' order by comentario ", cn, adOpenDynamic, adLockOptimistic
    ''' kenyo 09/11/2017 Mejora grupo comentario

    Do

        If mytablex.EOF Then Exit Do
        If "" & mytablex.Fields("grupoco") = buf Then
            I = I + 1
            mprodcod(I) = "" & mytablex.Fields("descripcio")
            wprodcod(I) = "" & mytablex.Fields("comentario")
            Else: Exit Do

        End If

        mytablex.MoveNext
    Loop

    mytablex.Close
    mprodtop = I
    mprodpag = 0

End Sub

Sub menu_producto(buf As String)

    Dim I As Integer

    Dim j As Integer

    Select Case buf

        Case "INI"
            mprodpag = 0

        Case "SIG"
            mprodpag = mprodpag + 11

            If mprodpag > 102 Then
                mprodpag = 0

            End If

        Case "ANT"
            mprodpag = mprodpag - 11

            If mprodpag < 0 Then
                mprodpag = 0

            End If

    End Select

    j = -1

    For I = mprodpag To 11 + mprodpag
        j = j + 1
        zproducto(j).Caption = mprodcod(I)
        wwprodcod(j) = wprodcod(I)
    Next I

End Sub

Private Sub zproducto_Click(Index As Integer)
    DATO = DATO + " " & Trim(zproducto(Index).Caption) & "*"
 
End Sub

